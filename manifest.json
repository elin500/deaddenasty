{
  "name": "hramoff",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [ "https://vk.com/*" ],
      "js": [ "script.js" ],
	  "run_at": "document_end"
    }
  ],
  "permissions": [
          "storage"
  ],
  "web_accessible_resources": [
    "/script.js"
  ]
}
